{
  "name": "@axiscommunications/fluent-topbar",
  "version": "8.21.0",
  "description": "Axis branded TopBar",
  "homepage": "https://github.com/AxisCommunications/fluent-components#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/AxisCommunications/fluent-components.git",
    "directory": "components/topbar"
  },
  "license": "MIT",
  "author": "Axis Communications AB",
  "exports": {
    ".": {
      "require": "./lib/cjs/index.js",
      "import": "./lib/index.js"
    }
  },
  "main": "lib/cjs/index.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "prebuild": "pnpm run -C ../../icons build",
    "build": "pnpm prebuild && pnpm build:cjs && pnpm build:esm",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "build:esm": "tsc",
    "check:unused-deps": "depcheck . --config=depcheck.yml",
    "lint": "tsc --noEmit && eslint . --cache",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.8",
    "@vitest/coverage-c8": "^0.33.0",
    "eslint": "^8.56.0",
    "jsdom": "^22.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.5.5",
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "@fluentui/react-components": "^9.47.1",
    "@fluentui/react-icons": "^2.0.233",
    "@fluentui/react-utilities": "^9.18.5",
    "react": ">=16.8.0 <19.0.0",
    "react-dom": ">=16.8.0 <19.0.0",
    "scheduler": "^0.20.0"
  },
  "dependencies": {
    "@axiscommunications/fluent-icons": "workspace:*"
  },
  "packageManager": "pnpm@8.1.0",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
