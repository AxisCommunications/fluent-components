{
  "lineWidth": 80,
  "typescript": {
    "arguments.trailingCommas": "never",
    "parameters.trailingCommas": "never",
    "module.sortImportDeclarations": "maintain"
  },
  "includes": ["**/*.{ts,tsx,cjs,mjs,json,md,yml,yaml}"],
  "excludes": [
    "**/dist",
    "**/lib",
    "**/generated",
    "**/docs",
    "**/node_modules",
    ".vscode",
    "icons/fonts",
    "icons/src",
    "icons/svgo_config.yml",
    "CHANGELOG.md",
    "changeset.md",
    "pnpm-lock.yaml"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.81.1.wasm",
    "https://plugins.dprint.dev/json-0.17.0.wasm",
    "https://plugins.dprint.dev/markdown-0.15.2.wasm",
    "https://plugins.dprint.dev/prettier-0.24.0.json@9a57d0d8e440ad90d07a503166af47e7a6a886abd46767933f9c279f72468596"
  ]
}
